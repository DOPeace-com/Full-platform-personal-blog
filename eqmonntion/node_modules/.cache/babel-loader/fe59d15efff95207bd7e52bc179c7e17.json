{"ast":null,"code":"import bus from \"@/util/Bus\"; //引入组件\n\nimport MyHeader from \"@/components/MyHeader\"; //导入Bootstrop-icon\n\nimport iconPath from 'bootstrap-icons/bootstrap-icons.svg';\nexport default {\n  name: 'App',\n  components: {\n    MyHeader\n  },\n\n  data() {\n    return {\n      title: \"Home\",\n      iconPath\n    };\n  },\n\n  created() {\n    bus.on('getTitle', val => {\n      this.title = val;\n    }); // 扩展API是否准备好，如果没有则监听“plusready\"事件\n\n    if (window.plus) {\n      this.plusReady();\n    } else {\n      document.addEventListener('plusready', this.plusReady, false);\n    }\n  },\n\n  methods: {},\n\n  beforeMount() {},\n\n  mounted() {}\n\n};","map":{"version":3,"mappings":"AAkBA,OAAOA,GAAP,MAAgB,YAAhB,C,CACA;;AACA,OAAOC,QAAP,MAAqB,uBAArB,C,CACA;;AACA,OAAOC,QAAP,MAAqB,qCAArB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAC;IAACH;EAAD,CAFE;;EAGbI,IAAI,GAAE;IACJ,OAAM;MACJC,KAAK,EAAC,MADF;MAEJJ;IAFI,CAAN;EAID,CARY;;EASbK,OAAO,GAAG;IACRP,GAAG,CAACQ,EAAJ,CAAO,UAAP,EAAmBC,GAAD,IAAO;MACvB,KAAKH,KAAL,GAAaG,GAAb;IACD,CAFD,EADQ,CAIR;;IACA,IAAIC,MAAM,CAACC,IAAX,EAAiB;MACf,KAAKC,SAAL;IACF,CAFA,MAEO;MACLC,QAAQ,CAACC,gBAAT,CAA0B,WAA1B,EAAuC,KAAKF,SAA5C,EAAuD,KAAvD;IACF;EACD,CAnBY;;EAoBbG,OAAO,EAAC,EApBK;;EAuBbC,WAAW,GAAG,CAEb,CAzBY;;EA0BbC,OAAO,GAAG,CAEV;;AA5Ba,CAAf","names":["bus","MyHeader","iconPath","name","components","data","title","created","on","val","window","plus","plusReady","document","addEventListener","methods","beforeMount","mounted"],"sourceRoot":"","sources":["C:\\Users\\章和平\\Documents\\DOP\\eqmonntion\\src\\App.vue"],"sourcesContent":["<template>\n  <MyHeader/>\n  <br>\n  <img width=\"120\" alt=\"MaxLogo\" src=\"./assets/logo.png\"/>\n  {{title}}\n  <svg class=\"bi\" width=\"32\" height=\"32\" fill=\"currentColor\">\n    <use :xlink:href=\"iconPath+'#heart-fill'\"/>\n  </svg>\n  <svg class=\"bi\" width=\"32\" height=\"32\" fill=\"currentColor\">\n    <use :xlink:href=\"iconPath+'#toggles'\"/>\n  </svg>\n  <svg class=\"bi\" width=\"32\" h