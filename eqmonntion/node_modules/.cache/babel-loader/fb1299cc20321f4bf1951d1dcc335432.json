{"ast":null,"code":"import bus from \"@/util/Bus\"; // import font from \"@/json/en-US_font.json\"\n\nimport iconPath from 'bootstrap-icons/bootstrap-icons.svg'; // 动态加载语言\n\nlet fontlanguage = navigator.language;\n\nlet footFile = require('@/json/' + fontlanguage + '_font.json');\n\nexport default {\n  name: 'MyHeader',\n\n  data() {\n    return {\n      WebName: footFile.WebName,\n      iconPath,\n      language: navigator.language,\n      windowsWidth: document.body.clientWidth\n    };\n  },\n\n  beforeMount() {\n    bus.on(\"getSettingLg\", val => {\n      this.fontlanguage = val;\n      footFile = require('@/json/' + fontlanguage + '_font.json');\n      console.log(footFile);\n    });\n  },\n\n  mounted() {\n    const _this = this; //监听window.body\n\n\n    window.onresize = () => {\n      _this.windowsWidth = document.body.clientWidth;\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,GAAP,MAAgB,YAAhB,C,CACA;;AACA,OAAOC,QAAP,MAAqB,qCAArB,C,CACA;;AACA,IAAIC,YAAW,GAAIC,SAAS,CAACC,QAA7B;;AACA,IAAIC,QAAO,GAAIC,OAAO,CAAC,YAAUJ,YAAV,GAAuB,YAAxB,CAAtB;;AAEA,eAAe;EACbK,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAE;IACJ,OAAO;MACLC,OAAO,EAACJ,QAAQ,CAACI,OADZ;MAELR,QAFK;MAGLG,QAAQ,EAACD,SAAS,CAACC,QAHd;MAILM,YAAY,EAACC,QAAQ,CAACC,IAAT,CAAcC;IAJtB,CAAP;EAMD,CATY;;EAUbC,WAAW,GAAG;IACZd,GAAG,CAACe,EAAJ,CAAO,cAAP,EAAuBC,GAAD,IAAO;MAC3B,KAAKd,YAAL,GAAoBc,GAApB;MACAX,QAAO,GAAIC,OAAO,CAAC,YAAUJ,YAAV,GAAuB,YAAxB,CAAlB;MACAe,OAAO,CAACC,GAAR,CAAYb,QAAZ;IACD,CAJD;EAKD,CAhBY;;EAiBbc,OAAO,GAAE;IACP,MAAMC,KAAI,GAAI,IAAd,CADO,CAEP;;;IACAC,MAAM,CAACC,QAAP,GAAkB,MAAM;MACtBF,KAAK,CAACV,YAAN,GAAqBC,QAAQ,CAACC,IAAT,CAAcC,WAAnC;IACF,CAFA;EAGD;;AAvBY,CAAf","names":["bus","iconPath","fontlanguage","navigator","language","footFile","require","name","data","WebName","windowsWidth","document","body","clientWidth","beforeMount","on","val","console","log","mounted","_this","window","onresize"],"sourceRoot":"","sources":["C:\\Users\\章和平\\Documents\\DOP\\eqmonntion\\src\\components\\MyHeader.vue"],"sourcesContent":["<template >\r\n  <div id=\"MyHeader\">\r\n    <div class=\"type-area\">\r\n      <div id=\"login\">\r\n        <img class=\"logoImg\" src=\"../assets/logo.png\">\r\n      </div>\r\n\r\n      <h1 style=\"display:inline-block;\">{{WebName}}</h1>\r\n      <!--This is the burger menu on a small screen-->\r\n      <svg class=\"bi-three-dots-vertical\" fill=\"currentColor\" v-if=\"windowsWidth<768\">\r\n        <use :xlink:href=\"iconPath+'#three-dots-vertical'\"/>\r\n      </svg>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport bus from \"@/util/Bus\"\r\n// import font from \"@/json/en-US_font.json\"\r\nimport iconPath from 'bootstrap-icons/bootstrap-icons.svg'\r\n// 动态加载语言\r\nlet fontlanguage = navigator.language;\r\nlet footFile = require('@/json/'+fontlanguage+'_font.json')\r\n\r\nexport default {\r\n  name: 'MyHeader',\r\n  data(){\r\n    return {\r\n      WebName:footFile.WebName,\r\n      iconPath,\r\n      language:navigator.language,\r\n      windowsWidth:document.body.clientWidth\r\n    }\r\n  },\r\n  beforeMount() {\r\n    bus.on(\"getSettingLg\",(val)=>{\r\n      this.fontlanguage = val\r\n      footFile = require('@/json/'+fontlanguage+'_font.json')\r\n      console.log(footFile)\r\n    })\r\n  },\r\n  mounted(){\r\n    const _this = this\r\n    //监听window.body\r\n    window.onresize = () => {\r\n      _this.windowsWidth = document.body.clientWidth\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n@import \"../less/main\";\r\n@media (min-width: 768px) {\r\n  .bi-three-dots-vertical:hover{\r\n    color: @hover-color;\r\n  }\r\n  .logoImg:hover{\r\n    transform: rotate(15deg);\r\n    filter: drop-shadow(0px 0px 5px @white);\r\n  }\r\n}\r\n#MyHeader{\r\n  .WebConfig(@row);\r\n  background-color:@bgColor_main;\r\n  padding: 0% 3%;\r\n  width: 94%;\r\n}\r\n.type-area{\r\n  .WebConfig(@row);\r\n  justify-content: space-between;\r\n  width: 100%;\r\n  max-width: @Max-type-with;\r\n}\r\nh1{\r\n  color:@white ;\r\n}\r\n.logoImg{\r\n  width: @logoSize;\r\n  height: @logoSize;\r\n}\r\n.logoImg:active{\r\n  transform: rotate(-15deg);\r\n  filter: drop-shadow(0px 0px 5px @shadow-blue);\r\n}\r\n.bi-three-dots-vertical{\r\n  width: @three-dots-vertical-size;\r\n  height: @three-dots-vertical-size;\r\n  color: @white;\r\n}\r\n.bi-three-dots-vertical:active{\r\n  color: @active-color;\r\n  transition: all .1s ease-out;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .logoImg{\r\n    width: @logoSize-small;\r\n    height: @logoSize-small;\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}