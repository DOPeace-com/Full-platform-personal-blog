nt-disable-next-line no-constant-condition\r\n    // this.acceptFontFile()\r\n  },\r\n  methods:{\r\n    /*\r\n    * 获取页面FontFild\r\n    * */\r\n    acceptFontFile(){\r\n      bus.on(\"getFontFile\",(val)=>{\r\n        this.fontFile = val\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n@import \"../../less/main\";\r\n@media (min-width: 768px) {\r\n  .Main_manu{\r\n    li:hover{\r\n      font-weight: 600;\r\n    }\r\n  }\r\n\r\n}\r\n.Main_manu{\r\n  .WebFlex(@row,\"flex-start\",\"\");\r\n  padding-left: 50px;\r\n  li{\r\n    font-size: 2em;\r\n    font-weight: 400;\r\n    color: @white;\r\n    padding: 0px 10px;\r\n    letter-spacing: 20px;\r\n  }\r\n}\r\n.cahngeLanguage{\r\n  font-size: 1.2px;\r\n}\r\n.toggle{\r\n  width: @toggle-size;\r\n  height: @toggle-size;\r\n}\r\n@media (max-width: 768px) {\r\n  .Main_manu{\r\n    .WebFlex(@column,\"flex-start\",\"\");\r\n    position: absolute;\r\n    left: 0;\r\n    top: @logoSize-small;\r\n    width: 100%;\r\n    height: 100%;\r\n    z-index: 99;\r\n    background-color: @bgColor_main;\r\n    padding-left: 10px;\r\n\r\n  }\r\n\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}