window.onresize = () =>{\r\n      console.log(document.body.clientWidth)\r\n    }\r\n    this.acceptFontFile()\r\n    this.accrptWindowWidth()\r\n    this.accrptSetSmallView()\r\n  },\r\n  methods: {\r\n    /*\r\n    * 获取页面FontFild\r\n    * */\r\n    acceptFontFile() {\r\n      bus.on(\"getFontFile\", (val) => {\r\n        this.fontFile = val\r\n      })\r\n    },\r\n    /*\r\n    * 发送网站显示的语言\r\n    * */\r\n    setLanguage(val) {\r\n      bus.emit('setLanguage', val)\r\n    },\r\n    /*\r\n    * 获取页面大小\r\n    * */\r\n    accrptWindowWidth() {\r\n      bus.on(\"getWindwoWidth\", (val) => {\r\n        this.windowsWidth = val\r\n      })\r\n    },\r\n    /*\r\n    * 监听控制小屏幕窗口的显示控制方法\r\n    * */\r\n    accrptSetSmallView(){\r\n      bus.on(\"setSmallVirw\",(val)=>{\r\n        this.smallView = val\r\n      })\r\n    }\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n@import \"../../less/main\";\r\n@media (min-width: 768px) {\r\n  .Main_manu{\r\n    li:hover{\r\n      font-weight: 600;\r\n    }\r\n  }\r\n\r\n}\r\n.Main_manu{\r\n  .WebFlex(@row,flex-start,center);\r\n  padding-left: 50px;\r\n  li{\r\n    font-size: 2em;\r\n    font-weight: 400;\r\n    color: @white;\r\n    padding: 0px 10px;\r\n    letter-spacing: 5px;\r\n  }\r\n}\r\n.cahngeLanguage{\r\n  .WebFlex(@row,flex-start,center);\r\n  font-size: 1.2em;\r\n  color: @white;\r\n  .toggle{\r\n    width: @toggle-size;\r\n    height: @toggle-size;\r\n    padding: 0px 5px;\r\n  }\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .SmailMenuTop{\r\n    .WebFlex(@row,center,center)\r\n  }\r\n  .bi-x{\r\n    color: @white;\r\n    width: @ico-size;\r\n    height: @ico-size;\r\n  }\r\n  .Main_manu{\r\n    .WebFlex(@column,flex-start,flex-start);\r\n    position: absolute;\r\n    left: 0;\r\n    top: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    z-index: 99;\r\n    background-color: @bgColor_main;\r\n    padding-left: 10px;\r\n    li{\r\n      width: 100%;\r\n    }\r\n  }\r\n\r\n}\r\n</style>\r\n"]},"metadata