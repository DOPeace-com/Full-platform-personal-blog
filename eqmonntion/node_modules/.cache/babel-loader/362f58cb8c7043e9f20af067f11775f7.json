{"ast":null,"code":"import iconPath from 'bootstrap-icons/bootstrap-icons.svg';\nimport bus from \"@/util/Bus\";\nexport default {\n  name: 'MyHeader',\n\n  data() {\n    return {\n      footFile: require('@/json/zh-CN_font.json'),\n      language: navigator.language,\n      iconPath,\n      windowsWidth: document.body.clientWidth\n    };\n  },\n\n  computed: {\n    fontlanguage: {\n      get() {\n        return \"en-US\";\n      },\n\n      set(val) {\n        console.log(val);\n        bus.emit('getWebLanguage', val);\n        this.footFile = require('@/json/' + val + '_font.json');\n      }\n\n    }\n  },\n\n  created() {\n    this.fontlanguage = this.language;\n  },\n\n  beforeMount() {},\n\n  mounted() {\n    const _this = this; //监听window.body\n\n\n    window.onresize = () => {\n      _this.windowsWidth = document.body.clientWidth;\n    }; //console.log(_this.fontlanguage,\"---\",this.language)\n\n  },\n\n  methods: {\n    changeFontlanguage(val) {\n      this.fontlanguage = val;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,QAAP,MAAqB,qCAArB;AACA,OAAOC,GAAP,MAAgB,YAAhB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,IAAI,GAAE;IACJ,OAAO;MACLC,QAAQ,EAACC,OAAO,CAAC,wBAAD,CADX;MAELC,QAAQ,EAACC,SAAS,CAACD,QAFd;MAGLN,QAHK;MAILQ,YAAY,EAACC,QAAQ,CAACC,IAAT,CAAcC;IAJtB,CAAP;EAMD,CATY;;EAUbC,QAAQ,EAAC;IACPC,YAAY,EAAC;MACXC,GAAG,GAAE;QACH,OAAO,OAAP;MACD,CAHU;;MAIXC,GAAG,CAACC,GAAD,EAAK;QACNC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACAf,GAAG,CAACkB,IAAJ,CAAS,gBAAT,EAA0BH,GAA1B;QACA,KAAKZ,QAAL,GAAgBC,OAAO,CAAC,YAAUW,GAAV,GAAc,YAAf,CAAvB;MACF;;IARW;EADN,CAVI;;EAsBbI,OAAO,GAAG;IACR,KAAKP,YAAL,GAAoB,KAAKP,QAAzB;EACD,CAxBY;;EAyBbe,WAAW,GAAG,CAEb,CA3BY;;EA4BbC,OAAO,GAAE;IACP,MAAMC,KAAI,GAAI,IAAd,CADO,CAEP;;;IACAC,MAAM,CAACC,QAAP,GAAkB,MAAM;MACtBF,KAAK,CAACf,YAAN,GAAqBC,QAAQ,CAACC,IAAT,CAAcC,WAAnC;IACF,CAFA,CAHO,CAMP;;EACD,CAnCY;;EAoCbe,OAAO,EAAC;IACNC,kBAAkB,CAACX,GAAD,EAAK;MACrB,KAAKH,YAAL,GAAoBG,GAApB;IACF;;EAHM;AApCK,CAAf","names":["iconPath","bus","name","data","footFile","require","language","navigator","windowsWidth","document","body","clientWidth","computed","fontlanguage","get","set","val","console","log","emit","created","beforeMount","mounted","_this","window","onresize","methods","changeFontlanguage"],"sourceRoot":"","sources":["C:\\Users\\章和平\\Documents\\DOP\\eqmonntion\\src\\components\\MyHeader.vue"],"sourcesContent":["<template >\r\n  <div id=\"MyHeader\">\r\n    <div class=\"type-area\">\r\n      <div id=\"login\">\r\n        <img class=\"logoImg\" src=\"../assets/logo.png\">\r\n      </div>\r\n\r\n      <h1 style=\"display:inline-block;\">{{footFile.WebName}}</h1>\r\n      <!--This is the burger menu on a small screen-->\r\n      <svg class=\"bi-three-dots-vertical\" fill=\"currentColor\" v-if=\"windowsWidth<768\">\r\n        <use :xlink:href=\"iconPath+'#three-dots-vertical'\"/>\r\n      </svg>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport iconPath from 'bootstrap-icons/bootstrap-icons.svg'\r\nimport bus from \"@/util/Bus\";\r\n\r\nexport default {\r\n  name: 'MyHeader',\r\n  data(){\r\n    return {\r\n      footFile:require('@/json/zh-CN_font.json'),\r\n      language:navigator.language,\r\n      iconPath,\r\n      windowsWidth:document.body.clientWidth\r\n    }\r\n  },\r\n  computed:{\r\n    fontlanguage:{\r\n      get(){\r\n        return \"en-US\"\r\n      },\r\n      set(val){\r\n        console.log(val)\r\n        bus.emit('getWebLanguage',val)\r\n        this.footFile = require('@/json/'+val+'_font.json')\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.fontlanguage = this.language\r\n  },\r\n  beforeMount() {\r\n\r\n  },\r\n  mounted(){\r\n    const _this = this\r\n    //监听window.body\r\n    window.onresize = () => {\r\n      _this.windowsWidth = document.body.clientWidth\r\n    }\r\n    //console.log(_this.fontlanguage,\"---\",this.language)\r\n  },\r\n  methods:{\r\n    changeFontlanguage(val){\r\n      this.fontlanguage = val\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n@import \"../less/main\";\r\n@media (min-width: 768px) {\r\n  .bi-three-dots-vertical:hover{\r\n    color: @hover-color;\r\n  }\r\n  .logoImg:hover{\r\n    transform: rotate(15deg);\r\n    filter: drop-shadow(0px 0px 5px @white);\r\n  }\r\n}\r\n#MyHeader{\r\n  .WebConfig(@row);\r\n  background-color:@bgColor_main;\r\n  padding: 0% 3%;\r\n  width: 94%;\r\n}\r\n.type-area{\r\n  .WebConfig(@row);\r\n  justify-content: space-between;\r\n  width: 100%;\r\n  max-width: @Max-type-with;\r\n}\r\nh1{\r\n  color:@white ;\r\n}\r\n.logoImg{\r\n  width: @logoSize;\r\n  height: @logoSize;\r\n}\r\n.logoImg:active{\r\n  transform: rotate(-15deg);\r\n  filter: drop-shadow(0px 0px 5px @shadow-blue);\r\n}\r\n.bi-three-dots-vertical{\r\n  width: @three-dots-vertical-size;\r\n  height: @three-dots-vertical-size;\r\n  color: @white;\r\n}\r\n.bi-three-dots-vertical:active{\r\n  color: @active-color;\r\n  transition: all .1s ease-out;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .logoImg{\r\n    width: @logoSize-small;\r\n    height: @logoSize-small;\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}