n\r\n<script>\r\n// import bus from \"@/util/Bus\";\r\n\r\nexport default {\r\n  name: \"MySlid\",\r\n  data(){\r\n    return{\r\n      flag:false,\r\n      oldX:0,\r\n      move:0,\r\n    }\r\n  },\r\n  mounted() {\r\n    this.binding()\r\n  },\r\n  methods:{\r\n    down(event){\r\n      this.flag = true\r\n      this.oldX = event.clientX\r\n    },\r\n    up(){\r\n      this.flag = false\r\n    },\r\n    movement(event){\r\n      /**\r\n       * 如果鼠标没有按下直接停止\r\n       */\r\n      if(!this.flag){\r\n        return\r\n      }\r\n      console.log(this.flag)\r\n      this.move = event.clientX - this.oldX\r\n      event.clientX = this.move\r\n    },\r\n    /*\r\n    * 绑定事件\r\n    * */\r\n    binding(){\r\n      this.$refs.testBox.addEventListener('mousedown',this.down,false)\r\n      document.body.addEventListener('mouseup',this.up,false)\r\n      this.$refs.testBox.addEventListener('mousemove',this.movement,false)\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n@import \"src/less/main\";\r\n.okcl{\r\n  position: absolute;\r\n  width: 40px;\r\n  height: 32px;\r\n  z-index: 3;\r\n  color: @white;\r\n  border: 1px #2ac845 solid;\r\n  background-color: #2ac845;\r\n}\r\n.box{\r\n  position: relative;\r\n  height: 34px;\r\n  text-align: center;\r\n  background-color: #eeeeee;\r\n}\r\n.btn{\r\n  position: absolute;\r\n  width: 40px;\r\n  height: 32px;\r\n  z-index: 3;\r\n  background-color:@white;\r\n  border: 1px @shadow solid;\r\n}\r\n.text{\r\n  position: relative;\r\n  z-index: 2;\r\n}\r\n.bg{\r\n  position: absolute;\r\n  top: 0;\r\n  z-index: 1;\r\n  width: 200px;\r\n  height: 34px;\r\n  background-color: @bgColor_main;\r\n}\r\n.test{\r\n  position: absolute;\r\n  width: 100px;\r\n  height: 100px;\r\n  background-color: #2ac845;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}