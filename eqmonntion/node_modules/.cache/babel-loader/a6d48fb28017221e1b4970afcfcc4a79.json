{"ast":null,"code":"export default {\n  name: \"MySlid\",\n\n  data() {\n    return {\n      ok: false,\n      check: 0,\n      downX: 0,\n      moveX: 0,\n      boxWidth: 0\n    };\n  },\n\n  mounted() {\n    this.boxWidth = document.querySelector(\".box\").clientWidth;\n  },\n\n  methods: {\n    /*\r\n    * 鼠标按下事件\r\n    * */\n    onmousedown(e) {\n      this.boxWidth = document.querySelector(\".box\").clientWidth;\n      this.downX = e.clientX;\n    },\n\n    /*\r\n    * 松开鼠标\r\n    * */\n    onmouseup(val) {\n      this.downX = 0;\n      this.ok = val;\n    },\n\n    /*\r\n    * 鼠标移动事件\r\n    * */\n    onmousemove(e) {\n      var move = e.clientX - this.downX;\n      var c = this.boxWidth - 40 - move;\n\n      if (c <= 0 && this.check >= this.boxWidth - 60) {\n        this.onmouseup(true);\n        return;\n      }\n\n      if (!this.downX || move <= 0) {\n        this.onmouseup(false);\n        return;\n      }\n\n      this.moveX = move;\n      this.check = move;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBA,eAAe;EACbA,IAAI,EAAE,QADO;;EAEbC,IAAI,GAAE;IACJ,OAAM;MACJC,EAAE,EAAC,KADC;MAEJC,KAAK,EAAE,CAFH;MAGJC,KAAI,EAAI,CAHJ;MAIJC,KAAI,EAAI,CAJJ;MAKJC,QAAO,EAAI;IALP,CAAN;EAOD,CAVY;;EAWbC,OAAO,GAAG;IACR,KAAKD,QAAL,GAAgBE,QAAQ,CAACC,aAAT,CAAuB,MAAvB,EAA+BC,WAA/C;EACD,CAbY;;EAcbC,OAAO,EAAC;IACN;;;IAGAC,WAAW,CAACC,CAAD,EAAG;MACZ,KAAKP,QAAL,GAAgBE,QAAQ,CAACC,aAAT,CAAuB,MAAvB,EAA+BC,WAA/C;MACA,KAAKN,KAAL,GAAaS,CAAC,CAACC,OAAf;IACD,CAPK;;IAQN;;;IAGAC,SAAS,CAACC,GAAD,EAAK;MACZ,KAAKZ,KAAL,GAAa,CAAb;MACA,KAAKF,EAAL,GAAUc,GAAV;IACD,CAdK;;IAeN;;;IAGAC,WAAW,CAACJ,CAAD,EAAG;MACZ,IAAIK,IAAG,GAAIL,CAAC,CAACC,OAAF,GAAU,KAAKV,KAA1B;MACA,IAAIe,IAAI,KAAKb,QAAL,GAAc,EAAd,GAAiBY,IAAzB;;MACA,IAAIC,CAAC,IAAE,CAAH,IAAQ,KAAKhB,KAAL,IAAY,KAAKG,QAAL,GAAc,EAAtC,EAAyC;QACvC,KAAKS,SAAL,CAAe,IAAf;QACA;MACF;;MACA,IAAI,CAAC,KAAKX,KAAN,IAAec,IAAI,IAAE,CAAzB,EAA2B;QACzB,KAAKH,SAAL,CAAe,KAAf;QACA;MACF;;MACA,KAAKV,KAAL,GAAaa,IAAb;MACA,KAAKf,KAAL,GAAae,IAAb;IACF;;EA/BM;AAdK,CAAf","names":["n