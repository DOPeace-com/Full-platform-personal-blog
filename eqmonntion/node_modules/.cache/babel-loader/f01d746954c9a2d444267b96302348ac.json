{"ast":null,"code":"import bus from \"@/util/Bus\"; //引入组件\n\nimport MyHeader from \"@/components/MyHeader\"; //导入Bootstrop-icon\n\nimport iconPath from 'bootstrap-icons/bootstrap-icons.svg';\nexport default {\n  name: 'App',\n  components: {\n    MyHeader\n  },\n\n  data() {\n    return {\n      title: \"Home\",\n      iconPath,\n      language: navigator.language\n    };\n  },\n\n  created() {\n    bus.on('getTitle', val => {\n      this.title = val;\n    });\n  },\n\n  methods: {\n    setLanguage(val) {\n      const _this = this;\n\n      if (val == null || val == \"\") {\n        //页面初次加载更具浏览器语言设定选择\n        _this.language = navigator.language;\n        bus.emit(\"getLageuage\", this.language);\n        return;\n      }\n\n      _this.language = val;\n      console.log(\"father is language\");\n      bus.emit(\"getLageuage\", _this.language);\n    }\n\n  },\n\n  beforeMount() {\n    this.setLanguage(\"en-US\");\n  },\n\n  mounted() {}\n\n};","map":{"version":3,"mappings":"AAkBA,OAAOA,GAAP,MAAgB,YAAhB,C,CACA;;AACA,OAAOC,QAAP,MAAqB,uBAArB,C,CACA;;AACA,OAAOC,QAAP,MAAqB,qCAArB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAC;IAACH;EAAD,CAFE;;EAGbI,IAAI,GAAE;IACJ,OAAM;MACJC,KAAK,EAAC,MADF;MAEJJ,QAFI;MAGJK,QAAQ,EAACC,SAAS,CAACD;IAHf,CAAN;EAKD,CATY;;EAUbE,OAAO,GAAG;IACRT,GAAG,CAACU,EAAJ,CAAO,UAAP,EAAmBC,GAAD,IAAO;MACvB,KAAKL,KAAL,GAAaK,GAAb;IACD,CAFD;EAGD,CAdY;;EAebC,OAAO,EAAC;IACNC,WAAW,CAACF,GAAD,EAAK;MACd,MAAMG,KAAI,GAAI,IAAd;;MACA,IAAIH,GAAE,IAAK,IAAP,IAAeA,GAAE,IAAK,EAA1B,EAA6B;QAC3B;QACAG,KAAK,CAACP,QAAN,GAAiBC,SAAS,CAACD,QAA3B;QACAP,GAAG,CAACe,IAAJ,CAAS,aAAT,EAAuB,KAAKR,QAA5B;QACA;MACF;;MACAO,KAAK,CAACP,QAAN,GAAiBI,GAAjB;MACAK,OAAO,CAACC,GAAR,CAAY,oBAAZ;MACAjB,GAAG,CAACe,IAAJ,CAAS,aAAT,EAAuBD,KAAK,CAACP,QAA7B;IACF;;EAZM,CAfK;;EA6BbW,WAAW,GAAG;IACZ,KAAKL,WAAL,CAAiB,OAAjB;EACD,CA/BY;;EAgCbM,OAAO,GAAG,CAEV;;AAlCa,CAAf","names":["bus","MyHeader","iconPath","name","components","data","title","language","navigator","created","on","val","methods","setLanguage","_this","e